{
    "opts": {
        "recurse": true,
        "template": "../template",
        "destination": "./docs/leaflet"
    },
    "disqus":"",
    "tags": {
        "allowUnknownTags": true,
        "linkdoc-leaflet":"http://leafletjs.com/reference-1.2.0.html",
	"linkdoc-htmlExtension":true
    },
    "source": {
        "includePattern": ".+\\.js?$",
        "excludePattern": ".+\\\\src\\\\common\\\\iServer\\\\.*Service.js?$",
        "include": [
            "src/common","src/leaflet"
        ]
    },
    "plugins": ["plugins/markdown","../plugins/factoryFunction","../plugins/linkdoc"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": false
        }
        //"applicationName": "iClient for Leaflet"
    }
}
